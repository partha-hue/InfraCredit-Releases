<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <group android:translateX="4" android:translateY="4">
        <!-- Main Blue Swoosh -->
        <path
            android:pathData="M15,75 C15,95 40,105 68,92 C85,82 92,65 88,45"
            android:strokeWidth="7"
            android:strokeLineCap="round">
            <aapt:attr name="android:strokeColor">
                <gradient
                    android:startX="15"
                    android:startY="90"
                    android:endX="90"
                    android:endY="50"
                    android:type="linear">
                    <item android:offset="0" android:color="#00AEEF"/>
                    <item android:offset="1" android:color="#0054A6"/>
                </gradient>
            </aapt:attr>
        </path>
        
        <!-- Inner Green Accent Line -->
        <path
            android:pathData="M22,82 Q45,98 78,78"
            android:strokeWidth="3.5"
            android:strokeLineCap="round"
            android:strokeColor="#8CC63F"/>

        <!-- Green Bar (Short) -->
        <path
            android:pathData="M32,78 L40,78 L40,58 L32,62 Z">
            <aapt:attr name="android:fillColor">
                <gradient
                    android:startX="32"
                    android:startY="78"
                    android:endX="40"
                    android:endY="58"
                    android:type="linear">
                    <item android:offset="0" android:color="#8CC63F"/>
                    <item android:offset="1" android:color="#A6D96A"/>
                </gradient>
            </aapt:attr>
        </path>
        
        <!-- Light Blue Bar (Medium) -->
        <path
            android:pathData="M44,78 L52,78 L52,43 L44,48 Z">
            <aapt:attr name="android:fillColor">
                <gradient
                    android:startX="42"
                    android:startY="78"
                    android:endX="52"
                    android:endY="43"
                    android:type="linear">
                    <item android:offset="0" android:color="#00AEEF"/>
                    <item android:offset="1" android:color="#33C1F2"/>
                </gradient>
            </aapt:attr>
        </path>

        <!-- Dark Blue Bar (Tall) -->
        <path
            android:pathData="M56,78 L64,78 L64,28 L56,33 Z">
            <aapt:attr name="android:fillColor">
                <gradient
                    android:startX="56"
                    android:startY="78"
                    android:endX="64"
                    android:endY="28"
                    android:type="linear">
                    <item android:offset="0" android:color="#0054A6"/>
                    <item android:offset="1" android:color="#0072BC"/>
                </gradient>
            </aapt:attr>
        </path>
            
        <!-- Sparkle Star -->
        <path
            android:pathData="M70,28 L71.5,30.5 L74,32 L71.5,33.5 L70,36 L68.5,33.5 L66,32 L68.5,30.5 Z"
            android:fillColor="#00AEEF"/>
    </group>

</vector>